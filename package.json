{
  "name": "fastify-postgres-template",
  "version": "1.0.0",
  "description": "",
  "readmeFilename": "README.md",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "build:start": "npm run build && npm run start",
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "dev": "cross-env NODE_ENV=development LOG_LEVEL=info nodemon | pino-pretty",
    "db:migrate": "npx prisma migrate deploy",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . && prettier -c \"**/*.json\" \"**/*.md\" \"**/*.yml\" \"src/**/*.ts\" --no-error-on-unmatched-pattern",
    "fmt": "prettier --write \"**/*.json\" \"**/*.md\" \"**/*.yml\" \"src/**/*.ts\" --no-error-on-unmatched-pattern",
    "prepare": "husky install"
  },
  "engines": {
    "node": "18 || 20"
  },
  "author": "Valerii Shandin <shandin.va@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^5.8.0",
    "@fastify/env": "^4.3.0",
    "@fastify/sensible": "^5.5.0",
    "@fastify/swagger": "^8.12.1",
    "@fastify/swagger-ui": "^2.0.1",
    "@fastify/type-provider-typebox": "^3.5.0",
    "@fastify/under-pressure": "^8.3.0",
    "@prisma/client": "^5.7.1",
    "@sinclair/typebox": "^0.31.28",
    "fastify": "^4.25.1",
    "fastify-plugin": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "fastify-tsconfig": "^2.0.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.3.0",
    "prettier": "^3.0.3",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "husky": "^8.0.0"
  }
}
